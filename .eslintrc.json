{
    "env": {
        "es2021": true,
        "node": true,
        "jest": true
    },
    "extends": [
        "standard"
    ],
    "parser": "@typescript-eslint/parser",
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },"plugins": [
        "@typescript-eslint"
    ],
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "rules": {
        "no-useless-constructor": "off",
        "no-new": "off",
        "no-unused-vars": "off",
        "@typescript-eslint/no-unused-vars": ["warn"]
    },
    "root": true
}